; Atoms
(const {nil} {})
(const {true} 1)
(const {false} 0)

; Function Definition
(def {fun} (\ {f b} {
  def (first f) (\ (tail f) b)
}))

; Constant Function Definition
(def {const_fun} (\ {f b} {
	def (first f) (\ (tail f) b)
}))

; Unpack List for function
(const_fun {unpack f l} {
	eval (join (list f) l)
})

; Pack List for Function
(const_fun {pack f & xs} {f xs})
